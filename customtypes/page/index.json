{
  "format": "page",
  "id": "page",
  "label": "Page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": ""
        }
      },
      "navigationLabel": {
        "type": "Text",
        "config": {
          "label": "Navigation label",
          "placeholder": "Label used in navigation menu"
        }
      },
      "tint": {
        "type": "Select",
        "config": {
          "label": "Tint",
          "placeholder": "",
          "options": ["primary", "secondary"],
          "default_value": "primary"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "media_banner": {
              "type": "SharedSlice"
            },
            "list_items": {
              "type": "SharedSlice"
            },
            "contacts": {
              "type": "SharedSlice"
            },
            "fifty_fifty_section": {
              "type": "SharedSlice"
            },
            "intro_block": {
              "type": "SharedSlice"
            },
            "testimonials_row": {
              "type": "SharedSlice"
            },
            "team": {
              "type": "SharedSlice"
            },
            "grid": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "metaTitle": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "metaDescription": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "metaImage": {
        "config": {
          "label": "Meta Image",
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
